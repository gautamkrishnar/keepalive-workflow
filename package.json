{
  "name": "keepalive-workflow",
  "version": "1.0.9",
  "description": "GitHub action to prevent GitHub from suspending your cronjob based trigger due to repository inactivity",
  "main": "library.js",
  "scripts": {
    "build-github": "parcel build  --no-source-maps --target=node --bundle-node-modules index.js",
    "lint": "jshint --exclude='node_modules/' *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gautamkrishnar/keepalive-workflow.git"
  },
  "keywords": [
    "github",
    "workflow",
    "keepalive",
    "action"
  ],
  "author": "Gautam krishna R <r.gautamkrishna@gmail.com> (https://www.github.com/gautamkrishnar)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gautamkrishnar/keepalive-workflow/issues"
  },
  "homepage": "https://github.com/gautamkrishnar/keepalive-workflow#readme",
  "dependencies": {
    "process": "latest"
  },
  "devDependencies": {
    "@actions/core": "^1.5.0",
    "jshint": "^2.12.0",
    "mocha": "^8.1.3",
    "parcel-bundler": "^1.12.4",
    "start-server-and-test": "^1.11.3"
  },
  "publishConfig": {
    "public": true
  }
}
